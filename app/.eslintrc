{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "typescript"
  ],
  "extends": "eslint-config-und",
  "rules": {
    "no-console": "off",
    "no-dupe-args": "off",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxBOF": 0,
        "maxEOF": 0
      }
    ]
  }
}
